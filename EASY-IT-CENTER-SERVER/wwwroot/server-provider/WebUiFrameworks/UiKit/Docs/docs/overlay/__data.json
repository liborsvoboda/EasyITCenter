             \u003Cul uk-tab=\"swiping: false\">\n                    \u003Cli>\u003Ca href=\"#\">Preview\u003C/a>\u003C/li>\n                    \u003Cli>\u003Ca href=\"#\">Markup\u003C/a>\u003C/li>\n                \u003C/ul>\n\n                \u003Cul class=\"uk-switcher uk-margin\">\n                    \u003Cli>\u003Cdiv class=\"uk-child-width-1-2@m\" uk-grid>\n    \u003Cdiv>\n\n        \u003Cdiv class=\"uk-inline\">\n            \u003Cimg src=\"images/photo.jpg\" width=\"1800\" height=\"1200\" alt=\"\">\n            \u003Cdiv class=\"uk-overlay uk-overlay-default uk-position-bottom\">\n                \u003Cp>Default Lorem ipsum dolor sit amet, consectetur adipiscing elit.\u003C/p>\n            \u003C/div>\n        \u003C/div>\n\n    \u003C/div>\n    \u003Cdiv>\n\n        \u003Cdiv class=\"uk-inline\">\n            \u003Cimg src=\"images/photo.jpg\" width=\"1800\" height=\"1200\" alt=\"\">\n            \u003Cdiv class=\"uk-overlay-default uk-position-cover\">\u003C/div>\n            \u003Cdiv class=\"uk-overlay uk-position-bottom uk-dark\">\n                \u003Cp>Default Lorem ipsum dolor sit amet, consectetur adipiscing elit.\u003C/p>\n            \u003C/div>\n        \u003C/div>\n\n    \u003C/div>\n\u003C/div>\u003C/li>\n                    \u003Cli>\u003Cpre>\u003Ccode id=\"code-example-1\">\u003Cspan class=\"hljs-tag\">&lt;\u003Cspan class=\"hljs-name\">div\u003C/span> \u003Cspan class=\"hljs-attr\">class\u003C/span>=\u003Cspan class=\"hljs-string\">&quot;uk-child-width-1-2@m&quot;\u003C/span> \u003Cspan class=\"hljs-attr\">uk-grid\u003C/span>&gt;\u003C/span>\n    \u003Cspan class=\"hljs-tag\">&lt;\u003Cspan class=\"hljs-name\">div\u003C/span>&gt;\u003C/span>\n\n        \u003Cspan class=\"hljs-tag\">&lt;\u003Cspan class=\"hljs-name\">div\u003C/span> \u003Cspan class=\"hljs-attr\">class\u003C/span>=\u003Cspan class=\"hljs-string\">&quot;uk-inline&quot;\u003C/span>&gt;\u003C/span>\n            \u003Cspan class=\"hljs-tag\">&lt;\u003Cspan class=\"hljs-name