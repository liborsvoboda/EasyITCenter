#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/typescript@3.8.3/node_modules/typescript/bin/node_modules:/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/typescript@3.8.3/node_modules/typescript/node_modules:/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/typescript@3.8.3/node_modules:/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/typescript@3.8.3/node_modules/typescript/bin/node_modules:/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/typescript@3.8.3/node_modules/typescript/node_modules:/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/typescript@3.8.3/node_modules:/mnt/e/Projekty/zEasy/netcoredevelop/typesctipt_playground/graphql-playground-main/packages/graphql-playground-html/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
